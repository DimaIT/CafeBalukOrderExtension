{
	"name": "Baluk order helper",
	"version": "0.3",
	"description": "Baluk order helper",
	"permissions": [
		"activeTab",
		"*://*.cafebaluk.by/",
		"*://cafebaluk.by/"
	],
	"content_scripts": [
		{
			"matches": ["*://*.cafebaluk.by/*", "*://cafebaluk.by/*"],
			"css": ["style.css"],
			"js": ["script.js"]
		}
	],
	"icons": {
		"16": "icon/32.png",
		"48": "icon/48.png",
		"128": "icon/256.png"
	},
	"manifest_version": 2
}
